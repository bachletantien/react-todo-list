{
  "name": "react-todo-list",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "set \"NODE_ENV=development\" && react-scripts start",
    "build": "set \"NODE_ENV=production\" && react-scripts build",
    "test:dev": "set \"NODE_ENV=development\" && cypress open",
    "test:prod": "set \"NODE_ENV=production\" && cypress open",
    "eject": "react-scripts eject",
    "server": "json-server --watch db.json --port 5000"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^8.7.0",
    "json-server": "^0.17.0"
  }
}
